oper = _items[1].Trim ();
float.TryParse ( _items[2], out value1 );

Console.ReadLine ();




List<string> GetItems(string text)
{
    List<string>items = line.Split(' ').ToList();

    foreach (var item in items.ToList ())
    {
        if (item == "")
        {
            items.Remove ( item );
        }
    }
    return items;
}
